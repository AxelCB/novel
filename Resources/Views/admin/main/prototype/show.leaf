#extend("admin/main/_layout")

#export("body") {
  <ol class="box breadcrumb">
    <li class="breadcrumb-item active">
      <a href="#adminPath("prototypes")">Prototypes</a>
    </li>
    <li class="breadcrumb-item active">#(prototype.name)</li>
  </ol>

  <form id="form" method="post" action="/admin/prototypes/#(prototype.id)" accept-charset="UTF-8">
    #embed("admin/main/prototype/form")
  </form>
}
